<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane prefHeight="500.0" prefWidth="800.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.emergency.controller.LogsViewController">
  <tabs>
    <Tab text="Incident Status Log">
         <content>
            <ListView fx:id="incidentStatusListView" />
         </content>
    </Tab>
    <Tab text="Unit Status Log">
         <content>
            <ListView fx:id="unitStatusListView" />
         </content>
    </Tab>
      <Tab text="Location History Log">
         <content>
            <ListView fx:id="locationHistoryListView" />
         </content>
      </Tab>
  </tabs>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</TabPane>